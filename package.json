{
  "name": "mos",
  "version": "0.0.0",
  "description": "Keeps your markdown files up to date",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "scripts": {
    "test": "mocha test/{**/,/}*.js",
    "coverage": "istanbul cover _mocha test/{**/,/}*.js -- -R spec",
    "precoveralls": "istanbul cover _mocha test/{**/,/}*.js --report lcovonly -- -R spec && npm i coveralls@2",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "postcoveralls": "rm -rf ./coverage",
    "md:update": "node index",
    "prepublish": "npm run md:update"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zkochan/mos"
  },
  "keywords": [
    "markdown",
    "md",
    "readme",
    "template",
    "generator",
    "documentation"
  ],
  "author": {
    "name": "Zoltan Kochan",
    "email": "zoltan.kochan@gmail.com",
    "url": "http://kochan.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zkochan/mos/issues"
  },
  "homepage": "https://github.com/zkochan/mos#readme",
  "dependencies": {
    "github-url-to-object": "2.2.1",
    "glob": "7.0.3",
    "read-pkg-up": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4"
  }
}
