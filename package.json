{
  "name": "mos",
  "version": "0.0.0",
  "description": "Keeps your markdown files up to date",
  "files": [
    "index.js",
    "lib",
    "plugins"
  ],
  "bin": "./lib/cli.js",
  "main": "index.js",
  "scripts": {
    "md:test": "node ./lib/cli test",
    "test": "npm run md:test && mocha lib/**/*.spec.js",
    "coverage": "istanbul cover _mocha lib/**/*.spec.js -- -R spec",
    "precoveralls": "istanbul cover _mocha lib/**/*.spec.js --report lcovonly -- -R spec && npm i coveralls@2",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "postcoveralls": "rm -rf ./coverage",
    "md:update": "node ./lib/cli",
    "prepublish": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zkochan/mos"
  },
  "keywords": [
    "markdown",
    "md",
    "readme",
    "template",
    "generator",
    "documentation"
  ],
  "author": {
    "name": "Zoltan Kochan",
    "email": "zoltan.kochan@gmail.com",
    "url": "http://kochan.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zkochan/mos/issues"
  },
  "homepage": "https://github.com/zkochan/mos#readme",
  "dependencies": {
    "async-regex-replace": "1.0.2",
    "chalk": "1.1.1",
    "github-url-to-object": "2.2.1",
    "glob": "7.0.3",
    "meow": "^3.7.0",
    "normalize-newline": "2.0.0",
    "read-pkg-up": "^1.0.1",
    "relative": "3.0.2",
    "run-async": "2.2.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4"
  }
}
