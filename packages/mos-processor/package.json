{
  "name": "mos-processor",
  "version": "1.2.2",
  "description": "A markdown processor for mos",
  "files": [
    "src/",
    "dist/"
  ],
  "main": "dist/es5/index.js",
  "jsnext:main": "dist/esnext/index.js",
  "typings": "dist/es5/index.d.ts",
  "scripts": {
    "pretest": "npm run tsc",
    "test": "mocha && npm run lint && node ../mos/bin/mos.js test",
    "lint": "tslint -c tslint.json ./src/**/*.ts",
    "precover": "npm run tsc",
    "cover": "istanbul cover -x \"**/*.spec.js\" node_modules/mocha/bin/_mocha -- -R spec",
    "postcover": "remap-istanbul -i coverage/esnext/coverage.raw.json -o coverage/ts/coverage.raw.json && remap-istanbul -i coverage/esnext/coverage.raw.json -o coverage/ts/lcov.info -t lcovonly && remap-istanbul -i coverage/esnext/coverage.raw.json -o coverage/ts/lcov-report -t html",
    "md": "node ../mos/bin/mos.js",
    "tsc": "tsc --pretty",
    "compile": "babel dist/esnext --out-dir dist/es5 --copy-files --source-maps",
    "prepublish": "npm run tsc && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mosjs/mos/tree/master/packages/mos-processor"
  },
  "keywords": [
    "mos",
    "markdown"
  ],
  "author": {
    "name": "Zoltan Kochan",
    "email": "zoltan.kochan@gmail.com",
    "url": "http://kochan.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mosjs/mos-processor/issues"
  },
  "homepage": "https://github.com/mosjs/mos-processor#readme",
  "dependencies": {
    "@types/node": "^4.0.25-alpha",
    "babel-runtime": "^6.9.0",
    "github-url-to-object": "^2.2.1",
    "magic-hook": "^2.0.2",
    "mos-core": "^1.0.1",
    "mos-read-pkg-up": "^1.0.0",
    "normalize-path": "^2.0.1",
    "remi": "^2.0.0-0",
    "remi-runner": "^1.1.3"
  },
  "devDependencies": {
    "@types/chai": "^3.4.25-alpha",
    "@types/mocha": "^2.2.24-alpha",
    "babel-cli": "^6.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.4.1",
    "istanbul": "^1.0.0-alpha.2",
    "markdownscript": "^1.3.0",
    "mocha": "^2.3.4",
    "plugiator": "^0.2.1",
    "remap-istanbul": "^0.6.4",
    "typescript": "^2.0.0-dev.20160703"
  },
  "engines": {
    "node": ">=0.12"
  }
}
