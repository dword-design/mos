{
  "name": "mos-plugin-ejs",
  "version": "1.1.0",
  "description": "A mos plugin that executes embedded js in markdown files",
  "files": [
    "src/",
    "dist/",
    "tonic-example.js"
  ],
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "test-es5": "mocha --compilers js:babel-register && npm run lint",
    "test:md": "npm run transpile && node ../mos/bin/mos.js test",
    "test": "npm run test-es5 && npm run test:md",
    "lint": "eslint src/**/*.js index.js",
    "cover": "istanbul cover -x \"**/*.spec.js\" node_modules/mocha/bin/_mocha --report lcovonly -- --compilers js:babel-register -R spec",
    "md": "npm run transpile && node ../mos/bin/mos.js",
    "transpile": "babel src --out-dir dist",
    "prepublish": "npm run transpile && tonic-example"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mosjs/mos/tree/master/packages/mos-plugin-ejs"
  },
  "keywords": [
    "mos-plugin",
    "js",
    "md"
  ],
  "author": {
    "name": "Zoltan Kochan",
    "email": "zoltan.kochan@gmail.com",
    "url": "http://kochan.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mosjs/mos-plugin-ejs/issues"
  },
  "homepage": "https://github.com/mosjs/mos-plugin-ejs#readme",
  "dependencies": {
    "babel-run-async": "^1.0.0",
    "babel-runtime": "^6.9.0",
    "github-url-to-object": "^2.2.1",
    "mos-read-pkg-up": "^1.0.0",
    "reserved-words": "^0.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.4.1",
    "eslint": "^2.9.0",
    "eslint-config-standard": "^5.3.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.3.4",
    "mos": "^1.3.0",
    "mos-processor": "^1.2.0",
    "tonic-example": "^0.2.0"
  },
  "engines": {
    "node": ">=0.12"
  }
}
